{
  "name": "@tzurot/ai-worker",
  "version": "3.0.0-beta.70",
  "private": true,
  "type": "module",
  "description": "AI processing worker service for Tzurot bot",
  "main": "dist/index.js",
  "scripts": {
    "clean": "find src -name '*.js' -type f -delete && find src -name '*.js.map' -type f -delete",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "pretest": "npm run clean",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "typecheck:spec": "tsc --noEmit --project tsconfig.spec.json"
  },
  "dependencies": {
    "@langchain/core": "^1.1.20",
    "@langchain/openai": "^1.2.6",
    "@tzurot/common-types": "workspace:*",
    "@tzurot/embeddings": "workspace:*",
    "bullmq": "^5.67.3",
    "ioredis": "^5.9.2",
    "langchain": "^1.2.19",
    "onnxruntime-node": "^1.24.1",
    "openai": "^6.19.0",
    "pino": "^10.3.1",
    "redis": "^5.10.0",
    "sharp": "^0.34.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@tzurot/test-utils": "workspace:*"
  }
}
