{
  "name": "@tzurot/common-types",
  "version": "3.0.0-beta.56",
  "private": true,
  "type": "module",
  "description": "Shared types and validation schemas for Tzurot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "find src -name '*.js' -type f -delete && find src -name '*.js.map' -type f -delete",
    "build": "tsc",
    "pretest": "npm run clean",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@prisma/client-runtime-utils": "^7.3.0",
    "discord-api-types": "^0.38.37",
    "discord.js": "^14.25.1",
    "ioredis": "^5.9.2",
    "lru-cache": "^11.2.5",
    "openai": "^6.16.0",
    "parse-duration": "^2.1.5",
    "pg": "^8.17.2",
    "pino": "^10.3.0",
    "redis": "^5.10.0",
    "tiktoken": "^1.0.22",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.3.0"
  }
}
