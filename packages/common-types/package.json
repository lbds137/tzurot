{
  "name": "@tzurot/common-types",
  "version": "3.0.0-beta.81",
  "private": true,
  "type": "module",
  "description": "Shared types and validation schemas for Tzurot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "find src -name '*.js' -type f -delete && find src -name '*.js.map' -type f -delete",
    "build": "tsc",
    "pretest": "npm run clean",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "typecheck:spec": "tsc --noEmit --project tsconfig.spec.json"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "discord-api-types": "^0.38.39",
    "discord.js": "^14.25.1",
    "ioredis": "^5.9.3",
    "lru-cache": "^11.2.6",
    "openai": "^6.22.0",
    "parse-duration": "^2.1.5",
    "pg": "^8.18.0",
    "pino": "^10.3.1",
    "redis": "^5.11.0",
    "tiktoken": "^1.0.22",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@tzurot/test-utils": "workspace:*",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.4.0"
  }
}
